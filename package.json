{
  "version": "0.1.0-alpha",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "prebuild": "tsc --noEmit && yarn lint && yarn test",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "react-dumb-form",
  "author": "Denis Kabana <github.com/deniskabana>",
  "repository": "https://github.com/deniskabana/react-dumb-form",
  "module": "dist/react-dumb-form.esm.js",
  "size-limit": [
    {
      "path": "dist/react-dumb-form.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-dumb-form.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
